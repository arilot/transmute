provider "aws" {
    access_key = "{{ aws_access_key }}"
    secret_key = "{{ aws_secrey_key }}"
    region = "{{ aws_region }}"
}

terraform {
  backend "s3" {
    bucket = "terraform.{{ clusterName }}"
    key    = "terraform.tfstate"
    region = "{{ aws_region }}"
    access_key = "{{ aws_access_key }}"
    secret_key = "{{ aws_secrey_key }}"

  }
}
