provider "aws" {
    access_key = "{{ aws_key }}"
    secret_key = "{{ aws_secret }}"
    region = "{{ aws_region }}"
}

terraform {
  backend "s3" {
    bucket = "terraform.{{ clusterName }}"
    key    = "terraform.tfstate"
    access_key = "{{ aws_key }}"
    secret_key = "{{ aws_secret }}"

  }
}
